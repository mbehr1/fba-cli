{
  "name": "fba-cli",
  "version": "1.0.3",
  "description": "A CLI (command line interface) to execute FBA (fishbone analysis) files with DLT-logs/adlt.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbehr1/fba-cli.git"
  },
  "homepage": "https://github.com/mbehr1/fba-cli#readme",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "fba-cli": "dist/index.js"
  },
  "scripts": {
    "build": "eslint src --ext .ts && tsc && node -e \"require('fs').cpSync('./package.json', './dist/package.json')\"",
    "lint": "eslint src --ext .ts",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "prepublish": "npm run build"
  },
  "keywords": [
    "fishbone",
    "analysis",
    "dlt"
  ],
  "author": "Matthias Behr",
  "license": "MIT",
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "semantic-release-npm-github-publish",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "json5": "2.2.3",
    "jsonpath": "^1.1.1",
    "mdast-util-assert": "^5.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "short-unique-id": "^5.0.3",
    "unist-builder": "^4.0.0",
    "unist-util-filter": "^5.0.1",
    "unist-util-inspect": "^8.0.0",
    "unist-util-is": "^6.0.0",
    "unist-util-map": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.4",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.8",
    "@types/jsonpath": "0.2.2",
    "@types/mdast": "^4.0.2",
    "@types/node": "^18.18.0",
    "@types/unist": "3.0.1",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "semantic-release": "^22.0.6",
    "semantic-release-npm-github-publish": "^1.5.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
